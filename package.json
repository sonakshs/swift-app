{
  "name": "SwiftApp",
  "version": "1.0.0",
  "homepage": "https://sonakshs.github.io/swift-app",
  "description": "An example of how to use typescript with preact-cli and all its goodies",
  "main": "index.js",
  "scripts": {
    "test": "eslint src && preact test",
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch"
  },
  "keywords": [
    "preact-cli",
    "typescript",
    "example",
    "preact",
    "webpack"
  ],
  "author": "Sonaksh Saraswat <sonaksh@live.com>",
  "contributors": [],
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-synacor": "^1.1.2",
    "gh-pages": "^2.0.1",
    "if-env": "^1.0.4",
    "preact-cli": "^2.2.1",
    "preact-cli-plugin-typescript": "^0.2.2",
    "preact-material-components": "^1.5.7",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "preact": "^8.4.2",
    "preact-compat": "^3.18.4",
    "preact-router": "^2.6.1",
    "react-scripts": "^3.0.1"
  }
}
